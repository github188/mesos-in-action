{
    "id": "docker-2048-created-by-api",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "alexwhen/docker-2048",
            "network": "BRIDGE",
            "portMappings": [
                { "containerPort": 80, "hostPort": 0 }
            ]
        }
    },
    "cpus": 0.5,
    "mem": 512.0,
    "instances": 1,
    "healthChecks": [
        {
            "protocol": "HTTP",
            "path": "/",
            "gracePeriodSeconds": 30,
            "intervalSeconds": 10,
            "portIndex": 0,
            "timeoutSeconds": 10,
            "maxConsecutiveFailures": 3
        }
    ]
}
